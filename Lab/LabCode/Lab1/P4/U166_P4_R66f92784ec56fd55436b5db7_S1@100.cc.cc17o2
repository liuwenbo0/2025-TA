#include "InsertionSort.h"
#include <vector>

void InsertionSort::mysort(std::vector<int>& nums) {
    int key;
    
    for (int j = 1; j < nums.size(); j++) {
        key = nums[j];
        int i = j - 1; 
        while (i >= 0 && nums[i] > key) {
            nums[i + 1] = nums[i];
            i = i - 1; 
        }
        nums[i + 1] = key; 
    }
}