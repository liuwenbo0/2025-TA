#include "InsertionSort.h"
void InsertionSort::mysort(std::vector<int>& nums) {
int i, j, point;
for (j = 1; j < nums.size(); j++)
{
    point = nums[j];
    for (i = j - 1; i >= 0; i--)
    {
        nums[i + 1] = nums[i];
        if (i == 0 && nums[i] > point)
        {
            nums[i] = point;
        }
        if (nums[i] < point)
        {
            nums[i + 1] = point;
            break;
        }
    }
}
}