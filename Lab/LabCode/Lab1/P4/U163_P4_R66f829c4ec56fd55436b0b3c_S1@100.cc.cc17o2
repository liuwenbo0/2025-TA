#include "InsertionSort.h"
// 通过插入排序对int队列nums进行升序排序
// @param
// nums: 完整的待排序队列，最终排序的结果应存放在nums中
using namespace std;
void InsertionSort::mysort(std::vector<int>& nums) {
    // 请在这里完成你的代码
    int j=1,lim=nums.end()-nums.begin();
    while (j<lim){
        int i=j-1;
        int key=nums[j];
        for(;i>=0 && nums[i]>key;i--){
            nums[i+1]=nums[i];
        }
        nums[i+1]=key;
        j++;
    }
}