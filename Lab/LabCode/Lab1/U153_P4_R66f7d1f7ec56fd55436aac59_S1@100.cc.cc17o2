#include "InsertionSort.h"

void InsertionSort::mysort(std::vector<int>& nums) {
    int i, j, key;
    for (i = 1; i < nums.size(); i++) {
        key = nums[i];
        j = i - 1;

        // 将 nums[i] 插入到已排序序列 nums[0...i-1] 中
        while (j >= 0 && nums[j] > key) {
            nums[j + 1] = nums[j];  // 将 nums[j] 向后移动
            j = j - 1;
        }
        nums[j + 1] = key;  // 将 key 插入到正确的位置
    }
}