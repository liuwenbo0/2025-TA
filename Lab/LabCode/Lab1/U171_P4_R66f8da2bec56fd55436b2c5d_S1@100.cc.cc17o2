#include "InsertionSort.h"
// 通过插入排序对int队列nums进行升序排序
// @param
// nums: 完整的待排序队列，最终排序的结果应存放在nums中
void InsertionSort::mysort(std::vector<int>& nums) {
    // 请在这里完成你的代码
    int N=nums.size(),b;
    for(int j=0;j<N-1;j++)
    {
        if(nums[j]>nums[j+1])
        {
            b=nums[j+1];
            int a;
            for(a=j;a>=0&&b<nums[a];a--)
            {
                nums[a+1]=nums[a];
            }
            nums[a+1]=b;
        }
    }
    return;
}