#include "InsertionSort.h"
// 通过插入排序对int队列nums进行升序排序
// @param
// nums: 完整的待排序队列，最终排序的结果应存放在nums中
void InsertionSort::mysort(std::vector<int>& nums) {
    // 请在这里完成你的代码
 
    for (auto it = nums.begin()+1;it!=nums.end();it++)
	{
		
        int key=*it;
        nums.erase(it); 
		auto i = it-1;
		while (i!=nums.begin()-1 && key<=*i)
		{i--;
		}
		nums.insert(i+1,key); 
	}
}