#include "InsertionSort.h"
// 通过插入排序对int队列nums进行升序排序
// @param
// nums: 完整的待排序队列，最终排序的结果应存放在nums中
void InsertionSort::mysort(std::vector<int>& nums) {
    // 请在这里完成你的代码
    int p;
    int n = nums.size();
    for(int i=0;i<n-1;i++)//排序第i个元素
    {
       for(int j=0;j<n-1-i;j++)//与第i个元素前的元素比较大小
       {
           if (nums[j]>nums[j+1])
           {
            p=nums[j+1];
            nums[j+1]=nums[j];
            nums[j]=p;
           }
       }
    }
}