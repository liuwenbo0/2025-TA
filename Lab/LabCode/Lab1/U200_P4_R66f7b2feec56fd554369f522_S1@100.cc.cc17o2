#include "InsertionSort.h"
// 通过插入排序对int队列nums进行升序排序
// @param
// nums: 完整的待排序队列，最终排序的结果应存放在nums中
void InsertionSort::mysort(std::vector<int>& nums) {
    int len=nums.size();
    for(int i=1;i<len;++i)
    {
        int j=i;
        while(j && nums[j]<nums[j-1])
        {
            nums[j]^=nums[j-1];
            nums[j-1]^=nums[j];
            nums[j]^=nums[j-1];
            --j;
        }
    }
}