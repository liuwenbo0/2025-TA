#include "InsertionSort.h"
// 通过插入排序对int队列nums进行升序排序
// @param
// nums: 完整的待排序队列，最终排序的结果应存放在nums中

void InsertionSort::mysort(std::vector<int>& nums) {
    // 请在这里完成你的代码
    int n = nums.size();
    for (int i = 0;i<=n-2;i++){
        int p = 0;
        int h =nums[i+1];
        for(int j = 0;j<=i;j++){
            if(h<=nums[j])
                break;
            p++;
        }
        for(int j = i;j>=p;j--){
            nums[j+1] =nums[j]; 
        }
        nums[p] = h;
    }    


}