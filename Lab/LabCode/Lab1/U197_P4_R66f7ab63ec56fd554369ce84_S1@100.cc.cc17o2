#include "InsertionSort.h"
// 通过插入排序对int队列nums进行升序排序
// @param
// nums: 完整的待排序队列，最终排序的结果应存放在nums中
void InsertionSort::mysort(std::vector<int>& nums)
{
	for (int i = 0; i < nums.size(); i++)
	{
		for (int j = i - 1; j >= 0; j--)
		{
			if (nums.at(j) > nums.at(j + 1))
			{
				int tmp = nums.at(j);
				nums.at(j) = nums.at(j + 1);
				nums.at(j + 1) = tmp;
			}
		}
	}
}