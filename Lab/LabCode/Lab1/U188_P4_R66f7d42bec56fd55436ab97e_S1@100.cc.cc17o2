#include "InsertionSort.h"
// 通过插入排序对int队列nums进行升序排序
// @param
// nums: 完整的待排序队列，最终排序的结果应存放在nums中
void InsertionSort::mysort(std::vector<int>& nums) {
    int n=nums.size();
    int a=0;
    for(int i=0;i<n;i++){
        int j=0;
        for(;j<i;j++){
            if(nums[j]<nums[i]){
                a++;
            }
        }
        int m=nums[j];
        for(int p=j;p>a;p--){
            nums[p]=nums[p-1];
        }
        nums[a]=m;
        a=0;
    }
}