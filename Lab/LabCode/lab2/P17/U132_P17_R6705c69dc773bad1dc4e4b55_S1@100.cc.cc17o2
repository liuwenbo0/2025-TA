#include "Solution.h"
#include <vector>

using namespace std;

vector<vector<int>> Solution::round_robin_schedule(int n) {
    vector<vector<int>> ans(n, vector<int>(n, 0));
    
    for(int i = 0; i< n; i++) {
        ans[i][0] = i + 1;
        for(int j = 1; j < n; j++) {
            // 按位异或
            int k = i ^ j;
            ans[i][j] = k + 1; 
        }
    }
    
    return ans;
}