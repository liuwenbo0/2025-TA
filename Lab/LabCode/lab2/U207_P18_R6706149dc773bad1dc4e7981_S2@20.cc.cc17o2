#include "Solution.h"
#include <vector>

vector<int> Solution::perfect_permutation(int n) 
{
    std::vector<int> ans;
    int i=1;
    while ( i <= n ) 
    {
        ans.push_back(i);
        i += 2;
    }
    i=2;
    while( i <= n ) 
    {
        ans.push_back(i);
        i += 2;
    }
    return ans;
}