#include "Solution.h"
vector<int> Solution::perfect_permutation(int n) {
    vector<int> ans;
    if(n==1){
      ans.push_back(1);}
    else{
      vector<int> ans1;
      vector<int> ans2;
      ans1=perfect_permutation((n+1)/2);
      ans2=perfect_permutation(n/2);
      for(int k=0;k<(n+1)/2;k++){
        ans.push_back(ans1[k]*2-1);}
      for(int k=0;k<n/2;k++){
        ans.push_back(ans2[k]*2);}}
return ans;}