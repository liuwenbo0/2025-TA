#include "Solution.h"
#include<vector>
using namespace std;
vector<int> Solution::perfect_permutation(int n) {
    vector<int> ans,odd,even;
    // 请在这里完成你的代码
    if(n>1)
    {
        odd=perfect_permutation((n+1)/2);
        even=perfect_permutation(n/2);
        for(int i:odd)
        {
            ans.push_back(2*i-1);
        }
        for(int i:even)
        {
            ans.push_back(2*i);
        }
    }
    else ans.push_back(1);

    
    return ans;
}