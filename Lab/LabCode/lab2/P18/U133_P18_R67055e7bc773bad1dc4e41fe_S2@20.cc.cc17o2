#include "Solution.h"
#include <vector>

using namespace std;

vector<int> Solution::perfect_permutation(int n) {
    vector<int> ans;
    
    // 添加奇数
    for (int i = 1; i <= n; i += 2) {
        ans.push_back(i);
    }
    
    // 添加偶数
    for (int i = 2; i <= n; i += 2) {
        ans.push_back(i);
    }
    
    return ans;
}