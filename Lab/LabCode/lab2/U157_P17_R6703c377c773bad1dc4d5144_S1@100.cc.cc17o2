#include "Solution.h"
#include <memory>
vector<vector<int>> Solution::round_robin_schedule(int n) {
    vector<vector<int>> ans;
    for(int i=0; i<n; i++) {
      ans.push_back(vector<int>(n, 0));
    }
    int k=2;
    ans[0][0]=ans[1][1]=1;
    ans[1][0]=ans[0][1]=2;
    while(k<n){
      for(int i=0; i<k; i++){
        for(int j=0; j<k; j++){
          ans[i+k][j+k]=ans[i][j];
          ans[i][j+k]=ans[i+k][j]=ans[i][j]+k;
        }
      }
      k=k*2;
    }
    return ans;
  }