#include "Solution.h"

vector<int> Solution::perfect_permutation(int n) {
    vector<int> ans;
    for (int i = 1; i <= n; ++i) {
        ans.push_back(i);
    }
    for (size_t i = 0; i < ans.size() / 2; ++i) {
        swap(ans[i * 2 + 1], ans[(ans.size() - 1 - i * 2)]);
    }
    return ans;
}