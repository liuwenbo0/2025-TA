#include<iostream>
#include<stdio.h>
using namespace std;

int findmax(int a[],int left,int right)
{  int max=-10000000;
   for(int i=left;i<=right;i++)
   {
     if (a[i]>max)
     max=a[i];
   }
  return max;
}
int main()
{
    int n=0;
    int k=0;
    cin>>n>>k;
    int a[10000];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    
    for(int i=0;i<n-k+1;i++)
    {   
        int max=findmax(a,i,i+k-1);
        printf("%d ",max);
       
    }
    return 0;

}