#include <iostream>
#include <deque>
#include <vector>

using namespace std;

int main() {
    int n, k;
    cin >> n >> k;
    vector<int> nums(n);
    for(int i = 0; i < n; i++) {
        cin >> nums[i];
    }

    deque<int> q;
    vector<int> ans;

    for(int i = 0; i < n; i++) {
        while(!q.empty() && q.front() <= i - k) {
            q.pop_front();
        }

        while(!q.empty() && nums[q.back()] < nums[i]) {
            q.pop_back();
        }
        q.push_back(i);

        if(i >= k - 1) {
            ans.push_back(nums[q.front()]);
        }
    }

    for(int i = 0; i < ans.size(); i++) {
        cout << ans[i];
        if(i < ans.size() - 1) {
            cout << " ";
        }
    }
    cout << endl;

    return 0;
}